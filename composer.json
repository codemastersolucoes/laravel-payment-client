{
    "name": "payments/client",
    "description": "Payment Client",
    "version": "0.3.0",
    "authors": [
        {
            "name": "Felipe da Silva Pinheiro",
            "email": "felipepinheiro@univicosa.com.br"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Payments\\Client\\Providers\\ClientServiceProvider"
            ],
            "aliases": {
                "Payment": "Payments\\Client\\Facades\\Payment"
            }
        }
    },
    "require": {
        "php": ">=7.0.0",
        "auth/openid-client": ">=0.7.3",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": ">=5.4",
        "barryvdh/laravel-cors": "^0.11.0"
    },
    "autoload": {
        "psr-4": {
            "Payments\\Client\\": ""
        }
    },
    "config": {
        "secure-http": false
    },
    "repositories": {
        "bitbucket": {
            "type": "vcs",
            "url": "http://bitbucket.univicosa.com.br:7990/scm/auth/openid-client.git"
        }
    }
}
